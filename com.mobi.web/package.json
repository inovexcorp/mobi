  "scripts": {
    "antlr": "rimraf ./src/main/resources/public/vendor/antlr4/dist && cd ./src/main/antlr4 && antlr4ts -o ../resources/public/vendor/antlr4/dist ./MOS.g4",
    "build:prod": "npm run build -- --env.env=prod",
    "build:dev": "npm run build -- --env.env=dev",
    "build": "npm run antlr && webpack --bail --progress",
    "test": "npm run test:ajs && npm run test:ng",
    "test:ajs": "karma start karma.conf.build.js --fail-on-skipped-tests",
    "test:ng": "karma start karma.conf.ng.js --build --fail-on-skipped-tests",
    "tdd": "karma start karma.conf.tdd.js",
    "tdd-ng": "karma start karma.conf.ng.js --browsers Chrome --reporters progress --no-single-run"
  },
